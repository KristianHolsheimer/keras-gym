    "import gym\n",
    "import keras_gym as km\n",
    "from tensorflow import keras\n",
    "K = keras.backend\n",
    "\n",
    "env = gym.make('CartPole-v0')\n",
    "\n",
    "\n",
    "# intial state\n",
    "s = env.reset()\n",
    "\n",
    "# draw action\n",
    "policy = km.RandomPolicy(env)\n",
    "a = policy(s)\n",
    "\n",
    "# get next state and reward\n",
    "s_next, r, done, info = env.step(a)\n",
    "\n",
    "s, s_next\n"
   ]
  },
